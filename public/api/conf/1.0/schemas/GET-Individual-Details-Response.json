{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Individual's Details in Breathing Space",
  "description": "Schema of GET Individual's Details for Breathing Space",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "details": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "nino": {
          "$ref": "Nino.json"
        },
        "dateOfBirth": {
          "$ref": "ISO-Date.json"
        }
      },
      "required": [
        "nino"
      ]
    },
    "nameList": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "name": {
          "type": "array",
          "minItems": 1,
          "items": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
              "firstForename": {
                "type": "string"
              },
              "secondForename": {
                "type": "string"
              },
              "surname": {
                "type": "string"
              }
            }
          }
        }
      },
      "required": [
        "name"
      ]
    },
    "addressList": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "address": {
          "type": "array",
          "minItems": 1,
          "items": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
              "addressLine1": {
                "type": "string"
              },
              "addressLine2": {
                "type": "string"
              },
              "addressLine3": {
                "type": "string"
              },
              "addressLine4": {
                "type": "string"
              },
              "addressLine5": {
                "type": "string"
              },
              "addressPostcode": {
                "type": "string"
              },
              "countryCode":  {
                "type": "integer"
              }
            }
          }
        }
      },
      "required": [
        "address"
      ]
    },
    "indicators": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "welshOutputInd": {
          "type": "number"
        }
      }
    }
  },
  "required": [ "details" ]
}
