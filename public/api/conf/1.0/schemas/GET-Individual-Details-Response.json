{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Individual's Details for Breathing Space",
  "description": "Schema of GET Individual's Details for Breathing Space",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "details": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "nino": {
          "type": "string",
          "pattern": "^(?:[ACEHJLMOPRSWXY][A-CEGHJ-NPR-TW-Z]|B[A-CEHJ-NPR-TW-Z]|G[ACEGHJ-NPR-TW-Z]|[KT][A-CEGHJ-MPR-TW-Z]|N[A-CEGHJL-NPR-SW-Z]|Z[A-CEGHJ-NPR-TW-Y])[0-9]{6}$",
          "example": "MZ006526"
        },
        "dateOfBirth": {
          "type": "string",
          "pattern": "^(19|20)[0-9]{2}-(0[1-9]|1[012])-(0[1-9]|[12][0-9]|3[01])$"
        }
      },
      "required": [
        "nino"
      ]
    },
    "nameList": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "name": {
          "type": "array",
          "minItems": 1,
          "items": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
              "firstForename": {
                "type": "string"
              },
              "secondForename": {
                "type": "string"
              },
              "surname": {
                "type": "string"
              }
            }
          }
        }
      },
      "required": [
        "name"
      ]
    },
    "addressList": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "address": {
          "type": "array",
          "minItems": 1,
          "items": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
              "addressLine1": {
                "type": "string"
              },
              "addressLine2": {
                "type": "string"
              },
              "addressLine3": {
                "type": "string"
              },
              "addressLine4": {
                "type": "string"
              },
              "addressLine5": {
                "type": "string"
              },
              "addressPostcode": {
                "type": "string"
              },
              "countryCode":  {
                "type": "integer"
              }
            }
          }
        }
      },
      "required": [
        "address"
      ]
    },
    "indicators": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "welshOutputInd": {
          "type": "number"
        }
      }
    }
  },
  "required": [ "details" ]
}
