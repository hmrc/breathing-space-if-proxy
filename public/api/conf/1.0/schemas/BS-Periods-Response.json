{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "GET Breathing Space Periods Response Schema",
  "description": "GET Breathing Space Periods Response Schema",
  "type": "object",
  "properties": {
    "periods": {
      "title" : "Periods",
      "type": "array",
      "minItems": 1,
      "uniqueItems": true,
      "additionalItems": false,
      "items": {
        "type": "object",
        "additionalProperties": false,
        "properties": {
          "periodID": {
            "type": "string",
            "description": "Identity of the Breathing Space Period",
            "pattern": "^[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12}$",
            "example": "c6743de1-28d4-43ab-9a26-978d2f5157b9"
          },
          "startDate": {
            "description": "Start date of a breathing space period",
            "$ref": "FullDate.json"
          },
          "endDate": {
            "description": "End date of a breathing space period",
            "$ref": "FullDate.json"
          }
        },
        "required": [
          "periodID",
          "startDate"
        ]
      }
    }
  },
  "required": [
    "periods"
  ],
  "additionalProperties": false
}
